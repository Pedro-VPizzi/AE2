#include <stdio.h>

#define TAMANHO 10

int main() {
    int A[TAMANHO];
    int M[TAMANHO];
    int X;

    printf("Digite os %d números do vetor A:\n", TAMANHO);

    for (int i = 0; i < TAMANHO; i++) {
        scanf("%d", &A[i]);
    }

    printf("Digite um número para X: ");
    scanf("%d", &X);

    for (int i = 0; i < TAMANHO; i++) {
        M[i] = A[i] * X;
    }

    printf("O vetor M, resultado da multiplicação de cada elemento de A por X, é:\n");

    for (int i = 0; i < TAMANHO; i++) {
        printf("%d ", M[i]);
    }

    printf("\n");

    return 0;
}
