#include <stdio.h>

#define TAMANHO 20

int main() {
    int numeros[TAMANHO];
    int i;

    printf("Digite 20 números:\n");

    for (i = 0; i < TAMANHO; i++) {
        scanf("%d", &numeros[i]);
    }

    printf("Números digitados na ordem inversa:\n");

    for (i = TAMANHO - 1; i >= 0; i--) {
        printf("%d ", numeros[i]);
    }

    return 0;
}
